<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="twoknightsgame.gui.controller.TwoKnightsMainController">
    <ImageView fitHeight="400.0" fitWidth="600.0">
        <image>
            <Image url="@../Images/startbackground.jpg" />
        </image>
    </ImageView>

    <Button id="startButton" fx:id="startButton" layoutX="248.0" layoutY="188.0" mnemonicParsing="false" onAction="#btnStartClicked" prefHeight="44.0" prefWidth="91.0" text="START">
      <font>
         <Font name="Impact" size="20.0" />
      </font></Button>
    <Button id="cancelButton" fx:id="cancelButton" layoutX="248.0" layoutY="278.0" mnemonicParsing="false" onAction="#btnCancelClicked" prefHeight="44.0" prefWidth="91.0" text="Cancel">
      <font>
         <Font name="Impact" size="20.0" />
      </font></Button>
   <Label layoutX="201.0" layoutY="23.0" prefWidth="199.0" text="Two Knights">
      <font>
         <Font name="Impact" size="40.0" />
      </font>
   </Label>
   <Label layoutX="255.0" layoutY="73.0" prefHeight="50.0" prefWidth="91.0" text="GAME">
      <font>
         <Font name="Impact" size="40.0" />
      </font>
   </Label>

</AnchorPane>
